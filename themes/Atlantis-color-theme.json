        // "keyword",
        "keyword.control",
				"storage.type",
        "storage.modifier",
        "punctuation.separator.key-value.html",
        "punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#C792EA"
			}
    },
    {
      "name": "Control (Import / Export)",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5FB3B3"
      }
