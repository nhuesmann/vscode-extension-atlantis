{
	"name": "Atlantis",
	"type": "dark",
	"colors": {
		"editor.background": "#222528",
		"editor.foreground": "#EEFFFF",
		"activityBarBadge.background": "#2976E3",
		"sideBarTitle.foreground": "#ABBAC9",
    "sideBar.background": "#222528",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#696969"
			}
		},
		{
      "name": "Semicolon, Colon, Period, Comma, String Begin/End",
			"scope": [
        "punctuation.terminator.statement",
        "punctuation.terminator.rule",
				"punctuation.separator.key-value",
				"punctuation.accessor",
        "punctuation.separator.comma",
        "punctuation.separator.parameter",
        "string constant.other.placeholder",
        "punctuation.definition.string",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "keyword.operator.type.annotation",
        // TODO: move these back to Keyword, Storage?
        "keyword",
        "storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#ABBAC9"
			}
    },
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
    },
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5C66"
			}
		},
